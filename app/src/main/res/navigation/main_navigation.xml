        android:id="@+id/startInspectionFragment"
        android:name="com.example.mobco.Ui.StartInspectionFragment"
        tools:layout="@layout/fragment_start_inspection"
        android:label="fragment_start_inspection">
        <action
            android:id="@+id/action_startInspectionFragment_to_PODetailsFragment"
            app:destination="@id/PODetailsFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
    </fragment>
    <fragment
        android:id="@+id/countingFragment"
        android:name="com.example.mobco.Ui.CountingFragment"
        tools:layout="@layout/fragment_counting"
        android:label="fragment_counting" />
    <action
        android:id="@+id/action_mainMenuFragment_to_putAwayFragment2"
        app:destination="@id/putAwayFragment"
        app:enterAnim="@anim/push_right_in"
        app:exitAnim="@anim/push_down_out"
        app:popEnterAnim="@anim/push_right_in"
        app:popExitAnim="@anim/push_up_out"/>
    <action
        android:id="@+id/action_startInspectionFragment_to_PODetailsFragment"
        app:destination="@id/PODetailsFragment"
        app:enterAnim="@anim/push_right_in"
        app:exitAnim="@anim/push_down_out"
        app:popEnterAnim="@anim/push_right_in"
        app:popExitAnim="@anim/push_up_out"/>
