            android:id="@+id/action_startReceiveFragment_to_PODetailsFragment"
            app:destination="@id/PODetailsFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
    </fragment>
    <action
        android:id="@+id/action_mainMenuFragment_to_countingFragment"
        app:destination="@id/countingFragment"
        app:enterAnim="@anim/push_right_in"
        app:exitAnim="@anim/push_down_out"
        app:popEnterAnim="@anim/push_right_in"
        app:popExitAnim="@anim/push_up_out"/>
