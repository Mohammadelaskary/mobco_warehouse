        android:label="fragment_p_o_details" />
    <action
        android:id="@+id/action_receivePOFragment_to_PODetailsFragment"
        app:destination="@id/PODetailsFragment"
        app:enterAnim="@anim/push_right_in"
        app:exitAnim="@anim/push_down_out"
        app:popEnterAnim="@anim/push_right_in"
        app:popExitAnim="@anim/push_up_out"/>
    <fragment
        android:id="@+id/receivingMainMenuFragment"
        android:name="com.example.mobco.Ui.ReceivingMenuFragment"
        tools:layout="@layout/fragment_receiving_menu"
        android:label="fragment_receiving_main_menu">
        <action
            android:id="@+id/action_mainMenuFragment_to_receivePOFragment"
            app:destination="@id/receivePOFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
        <action
            android:id="@+id/action_mainMenuFragment_to_inspectionFragment"
            app:destination="@id/inspectionFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
        <action
            android:id="@+id/action_mainMenuFragment_to_putAwayFragment"
            app:destination="@id/putAwayFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
        <action
            android:id="@+id/action_mainMenuFragment_to_countingFragment"
            app:destination="@id/countingFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
        <action
            android:id="@+id/action_mainMenuFragment_to_putAwayFragment2"
            app:destination="@id/putAwayFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>

