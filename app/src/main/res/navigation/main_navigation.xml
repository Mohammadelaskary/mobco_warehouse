        android:name="com.example.mobco.Ui.ReceivePOFragment"
        tools:layout="@layout/fragment_receive_p_o"
        android:label="fragment_receive_p_o">
        <action
            android:id="@+id/action_receivePOFragment_to_PODetailsFragment"
            app:destination="@id/PODetailsFragment"
            app:enterAnim="@anim/push_right_in"
            app:exitAnim="@anim/push_down_out"
            app:popEnterAnim="@anim/push_right_in"
            app:popExitAnim="@anim/push_up_out"/>
